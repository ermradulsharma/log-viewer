{
    "name": "skywalker-labs/log-viewer",
    "description": "A powerful and highly customizable Log Viewer for Laravel, featuring a robust API, Role-Based Access Control (RBAC), AI-powered error analysis, performance insights, and automated reporting. Supports Laravel 5.x to 12.x with dark mode and multi-language support out of the box.",
    "type": "library",
    "license": "MIT",
    "version": "1.1.0",
    "keywords": [
        "laravel",
        "log-viewer",
        "log",
        "monitoring",
        "ai-analysis",
        "rbac",
        "performance-insights",
        "reporting",
        "api",
        "multi-language",
        "dark-mode",
        "laravel-package",
        "skywalker"
    ],
    "homepage": "https://github.com/skywalker-labs/log-viewer",
    "support": {
        "issues": "https://github.com/skywalker-labs/log-viewer/issues",
        "source": "https://github.com/skywalker-labs/log-viewer"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/ermradulsharma"
        }
    ],
    "authors": [
        {
            "name": "Skywalker Labs Team",
            "email": "skywalkerlknw@gmail.com",
            "homepage": "https://skywalker-labs.com/",
            "role": "Developer"
        },
        {
            "name": "Mradul Sharma",
            "email": "skywalkerlknw@gmail.com",
            "homepage": "https://mradulsharma.vercel.app/",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ermradulsharma/laravel-toolkit": "^1.0|^2.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "laravel/framework": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Skywalker\\LogViewer\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Skywalker\\LogViewer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test:dox": "phpunit --testdox --colors=always",
        "test:cov": "phpunit --coverage-html build/coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skywalker\\LogViewer\\LogViewerServiceProvider",
                "Skywalker\\LogViewer\\Providers\\DeferredServicesProvider"
            ],
            "aliases": {
                "LogViewer": "Skywalker\\LogViewer\\Facades\\LogViewer"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ermradulsharma/log-viewer.git"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}